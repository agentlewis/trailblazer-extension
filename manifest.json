{
  "manifest_version": 2,
  "name": "Trailblazer",
  "version": "0.0.11",
  "permissions": [
    "*://api.keen.io/",
    "https://app.trailblazer.io/",
    "http://staging.trailblazer.io/",
    "http://localhost:3000/",
    "chrome://favicon/",
    "*://*/",
    "identity",
    "storage",
    "tabs",
    "webRequest",
    "windows"
  ],
  "icons": {
    "16": "src/ui/icons/16.png",
    "48": "src/ui/icons/48.png",
    "128": "src/ui/icons/128.png"
  },
  "background": {
    "scripts": [
      "src/lib/underscore.js",
      "src/lib/superagent.js",
      "src/lib/promise-5.0.0.js",
      "src/adapter/chrome_event_adapter.js",
      "src/adapter/chrome_identity_adapter.js",
      "src/adapter/trailblazer_http_storage_adapter.js",
      "build/state-manager.js",
      "build/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "/src/ui/icons/19-unknown.png",
      "38": "/src/ui/icons/38-unknown.png"
    },
    "default_popup": "/src/ui/popup/unknown.html"
  }
}
